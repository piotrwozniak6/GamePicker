@model List<Category>

<h1>Categories</h1>
<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>
            Category
        </th>
        <th>
            Order
        </th>
    </tr>
    </thead>
    <tbody>
    @foreach (var obj in Model.OrderBy(u => u.Order))
    {
    <tr>
        <td>@obj.Name</td>
        <td>
            @obj.Order
        </td>
    </tr>
    }
    </tbody>
</table>